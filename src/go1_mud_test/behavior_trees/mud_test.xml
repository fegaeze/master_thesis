<root BTCPP_format="4">
    <BehaviorTree ID="MainTree">
        <ReactiveSequence name="root_sequence">
            <Sequence>
                <RobotStateReceived name="robot_state_received?"/>
                <Fallback>
                    <RobotInitialized name="robot_initialized?"/>
                    <RobotInitialization name="robot_initialization_action" />
                </Fallback>
            </Sequence>

            <ReactiveFallback>
                <ReactiveSequence>
                    <StandKeyPressed name="go1_stand_key_pressed?" />
                    <RobotStandAction name="go1_stand_action"/>
                </ReactiveSequence>

                <ReactiveSequence>
                    <LieDownKeyPressed name="go1_lie_down_pressed?"/>
                    <RobotLieDownAction name="go1_lie_down_action"/>
                </ReactiveSequence>

                <ReactiveSequence>
                    <DropFootKeyPressed name="go1_drop_foot_pressed?"/>
                    <RobotDropFootAction name="go1_drop_foot_action"/>
                </ReactiveSequence>

                <ReactiveSequence>
                    <FrRaiseKeyPressed name="go1_fr_raise_pressed?"/>
                    <Sequence>
                        <RobotGoToCogAction name="go1_go_to_cog_action"/>
                        <RobotFrRaiseAction name="go1_fr_raise_action"/>
                    </Sequence>
                </ReactiveSequence>
            </ReactiveFallback>
            
        </ReactiveSequence>
    </BehaviorTree>
</root>